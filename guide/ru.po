#: ./en/concept/aliases.md:21
#, fuzzy, no-wrap
msgid ""
"    'yiisoft/aliases' => [\n"
"        'aliases' => [\n"
"            // ...\n"
msgstr ""
"    'yiisoft/aliases' => [\n"
"        'aliases' => [\n"
"            // ...\n"

